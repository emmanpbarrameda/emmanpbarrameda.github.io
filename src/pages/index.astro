---
import QuoteSection from "../components/quote/quote-section.astro";
import AboutMeSection from "../components/about-me/about-me-section.astro";
import SkillsSection from "../components/skills/skills-section.astro";
import CertificatesSection from "../components/certificates/certificates-section.astro";
import BlogMainSection from "../components/blog/blog-main-section.astro";
import ContactSection from "../components/contact/contact-section.astro";
import CredentialsSection from "../components/credentials/credentials-section.astro";
import Footer from "../components/footer/footer.astro";
import Header from "../components/header/header.astro";
import HeroSection from "../components/hero/hero-section.astro";
import ProjectsSection from "../components/projects/projects-section.astro";
import MainLayout from "../layouts/main-layout.astro";
import { infos } from "../data/infos.js";
---

<MainLayout title={infos.main_title} description={infos.main_description}>
  <Header />
  <main>
    <HeroSection />
    <QuoteSection />
    <AboutMeSection />
    <!-- <ClientsSection /> -->
    <SkillsSection />
    <ProjectsSection />
    <CredentialsSection />
    <CertificatesSection />
    <BlogMainSection />
    <ContactSection />
  </main>
  <Footer />
</MainLayout>
